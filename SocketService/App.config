<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
  </configSections>
  <superSocket>
    <servers>
      <!-- 可以配置多个Server-->
      <server name="ChatSocket" textEncoding="gb2312" serverType="SocketService.ChatServer, SocketService" ip="Any" port="1001" maxConnectionNumber="100"></server>
    </servers>
  </superSocket>
  <runtime>
    <gcServer enabled="true" />
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
</configuration>